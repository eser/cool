{
  "lock": false,
  "tasks": {
    "test": "deno test -A --parallel --unstable",
    "fixture": "deno run -A --watch=static/,routes/ tests/fixture/dev.ts",
    "www": "deno task --cwd=www start",
    "screenshot": "deno run -A www/utils/screenshot.ts",
    "check:types": "deno check **/*.ts && deno check **/*.tsx",
    "ok": "deno fmt --check && deno lint && deno task check:types && deno task test"
  },
  "exclude": [
    "**/_lime/*"
  ],
  "imports": {
    "$cool/": "../",
    "$std/": "https://deno.land/std@0.203.0/",

    "@preact/signals-core": "npm:@preact/signals-core@1.4.0",

    "preact": "npm:preact@10.17.1",
    "preact/": "npm:/preact@10.17.1/",
    "preact-render-to-string": "npm:preact-render-to-string@6.2.1",
    "@preact/signals": "npm:@preact/signals@1.2.1",

    "_react": "npm:preact@10.17.1/compat",
    "_react-dom": "npm:preact@10.17.1/compat",
    "_react/": "npm:/preact@10.17.1/",
    "_@preact/signals-react": "npm:@preact/signals@1.2.1",
    "react": "npm:react@18.2.0",
    "react/": "npm:/react@18.2.0/",
    "react-dom": "npm:react-dom@18.2.0",
    "react-dom/": "npm:/react-dom@18.2.0/",
    "@preact/signals-react": "npm:@preact/signals-react@1.3.6"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
